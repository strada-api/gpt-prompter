{
  "name": "gpt-prompter",
  "version": "0.0.1",
  "description": "Developer utilities for prompt engineering",
  "keywords": [
    "gpt",
    "typescript"
  ],
  "main": "dist/index.js",
  "homepage": "https://github.com/strada-api/gpt-prompter",
  "bugs": {
    "url": "https://github.com/strada-api/gpt-prompter/issues"
  },
  "author": "Udayan Maurya <udayan@getstrada.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strada-api/gpt-prompter.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-exports": "attw --pack .",
    "build": "tsup",
    "ci": "pnpm run build && pnpm run check-format && pnpm run check-exports"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}
